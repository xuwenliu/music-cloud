.player-container{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    filter:blur(40rpx);
    opacity: 0.5;
    z-index: -1;
}
.player-mask{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -2;
    background-color: #222;
}

.player-info{
    text-align: center;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
}

.player-disc{
    width: 592rpx;
    height: 592rpx;
    background: url('data:image/png;base64,') no-repeat center/contain;
    position: absolute;
    left: 50%;
    transform: translate(-50%,140rpx);
}
.player-disc::after{
    content: '';
    width: 192rpx;
    height: 274rpx;
    position: absolute;
    top: -150rpx;
    left: 266rpx;
    background: url('data:image/png;base64,') no-repeat center/contain;
    transform: rotate(-15deg);
    transform-origin: 24rpx 10rpx;
    transition: transform .5s ease;
}

.play.player-disc::after{
    transform: rotate(0deg);
}


.player-img{
    width: 368rpx;
    height: 368rpx;
    border-radius: 50%;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -184rpx;
    margin-left: -184rpx;
}

/* 播放控件样式 */
.control {
    position: absolute;
    left: 50%;
    bottom: 8%;
    transform: translateX(-50%);
    display: flex;
    align-items: center;
}
.control .iconfont {
    color: #fefefe;
}

.icon-shangyishoushangyige,.icon-xiayigexiayishou{
    font-size: 80rpx;
}

.icon-bofang1,.icon-zanting1{
    font-size: 120rpx;
    margin: 0 50rpx;
}

.rotation{
    animation: rotation 12s linear infinite;
    -moz-animation: rotation 12s linear infinite;
    -webkit-animation: rotation 12s linear infinite;
    -o-animation: rotation 12s linear infinite;
}

@keyframes rotation{
    from{
        transform: rotate(0deg);
    }
    to{
        transform: rotate(360deg);
    }
}

/* 播放动画暂停到某一帧 */
.rotation-paused{
    animation-play-state: paused;
}


/* 进度条 */
.progress-bar{
    width: 90%;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 24%;
}